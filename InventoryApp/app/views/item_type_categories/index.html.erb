<p id="notice"><%= notice %></p>

<h1>Listing Item Type Categories</h1>

<table>
  <thead>
    <tr>
      <th>Itemtypecategory</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @item_type_categories.each do |item_type_category| %>
      <tr>
        <td><%= item_type_category.itemtypecategory %></td>
        <td><%= link_to 'Show', item_type_category %></td>
        <td><%= link_to 'Edit', edit_item_type_category_path(item_type_category) %></td>
        <td><%= link_to 'Destroy', item_type_category, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Item type category', new_item_type_category_path %>
